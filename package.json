{
  "name": "nestjs-prisma-querybuilder",
  "author": "Hariel Thums <hariel@hotmail.com>",
  "version": "1.1.0",
  "license": "MIT",
  "description": "#",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/HarielThums/nestjs-prisma-query-builder#readme",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HarielThums/nestjs-prisma-query-builder.git"
  },
  "bugs": {
    "url": "https://github.com/HarielThums/nestjs-prisma-query-builder/issues"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/express": "^4.17.14",
    "@types/jest": "27.0.2",
    "@types/node": "^16.18.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^27.5.1",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.0",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
