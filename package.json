{
  "name": "nestjs-prisma-querybuilder",
  "author": "Hariel Thums <hariel@hotmail.com>",
  "version": "1.2.1",
  "license": "MIT",
  "description": "#",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/HarielThums/nestjs-prisma-query-builder#readme",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HarielThums/nestjs-prisma-query-builder.git"
  },
  "bugs": {
    "url": "https://github.com/HarielThums/nestjs-prisma-query-builder/issues"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --loglevel=error --cache --write \"src/**/*.ts\"",
    "lint": "eslint --cache \"{src,apps,libs,test}/**/*.ts\" --fix",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.8",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.15",
    "@types/jest": "29.2.5",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
